    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "ejs": "^3.1.2",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-handlebars": "^4.0.3",
    "express-session": "^1.17.1",
    "express-validator": "^6.4.0",
    "firebase-functions": "^3.6.1",
    "greenlock-express": "^4.0.3",
    "gridfs-stream": "^1.1.1",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "mongoclient": "^1.0.3",
    "mongodb": "^3.6.3",
    "mongoose": "^5.9.10",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.3",
    "selfsigned": "^1.10.8",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socket.io-server": "^1.0.0-b",
    "ts-mongoose": "0.0.24",
    "ts-node": "^8.9.1"
  },
