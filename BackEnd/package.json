{
  "name": "nodejsworks",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "delete": "rmdir /s /q \"./dist/\" || true",
    "dev": "npm run watch",
    "build": "tsc -p .",
    "build-start": "npm run build && node dist/server.js",
    "build-fresh-start": "npm run delete && npm run build && node dist/server.js",
    "build-start-old": "npm run build && node dist/OldModelsAndRoutes/server.js",
    "database-fresh": "npm run delete && npm run build && node dist/MongoDBPopulate/run.js",
    "database": "npm run build && node dist/MongoDBPopulate/run.js",
    "keyList": " tsc ./src/MemoryScripts/UserMemoryDetails.ts ./src/MemoryScripts/run.ts && node ./src/MemoryScripts/run.js",
    "keyList-async": "(rm ./src/MemoryScripts/*.js || true ) && tsc --esModuleInterop ./src/MemoryScripts/TestUserProfileModel.ts ./src/MemoryScripts/UniversityClubMemoryDetails.ts ./src/MemoryScripts/UserMemoryDetails_log.ts ./src/MemoryScripts/ConnectThread.ts ./src/MemoryScripts/runOnThread.ts ./src/MemoryScripts/run.ts && node ./src/MemoryScripts/run.js",
    "start": "node dist/server.js",
    "stop": "node dist/server.stop.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-watch": "npm run delete && npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "(rm \"./src/TestScripts/*.js\" ||true) && tsc ./src/TestScripts/script1.ts ./src/TestScripts/run.ts && node src/TestScripts/run.js",
    "watch-ts": "tsc -w"
  },
  "author": "",
  "license": "ISC",
